{
  "name": "tms",
  "version": "1.1.3",
  "description": "TMS or Transportation Management System is a HZN properiatery fleet management software",
  "private": true,
  "engines": {
    "node": "^16.13.0"
  },
  "repository": "https://github.com/HZN-one/tms",
  "scripts": {
    "start": "node -r dotenv/config build/index.js",
    "start:dev": "nodemon --config restart.json",
    "clean": "rm -rf coverage build tmp",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --colors --verbose --coverage",
    "test:dev": "jest --colors --verbose --coverage --maxWorkers=30%",
    "cloud:build": "gcloud builds submit --tag gcr.io/hzn-one/tms-development",
    "cloud:run": "gcloud run deploy tms-development --image gcr.io/hzn-one/tms-development --allow-unauthenticated --region us-central1",
    "cloud:run:new": "npm run cloud:build && npm run cloud:run",
    "docker:build": "docker build -t tms-development --build-arg project_id=hzn-one --build-arg app_env=development .",
    "docker:run": "docker run -p 8080:8080 tms-development",
    "docker:run:new": "npm run docker:build && npm run docker:run",
    "migrate:up": "ts-node -r tsconfig-paths/register -r dotenv/config migrate up",
    "migrate:down": "ts-node -r tsconfig-paths/register -r dotenv/config migrate down --last",
    "migrate:new": "ts-node -r tsconfig-paths/register -r dotenv/config migrate new",
    "migrate:status": "ts-node -r tsconfig-paths/register -r dotenv/config migrate status"
  },
  "author": "HZN",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^5.18.1",
    "@hzn-one/commons": "^1.0.55",
    "@hzn-one/task-creators": "^1.0.18",
    "ansi-regex": "^6.0.1",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "firebase-admin": "^10.0.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "joi-password": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.34",
    "module-alias": "^2.2.2",
    "mongo-migrate-ts": "^1.0.16",
    "mongoose": "^6.1.4",
    "multer": "^1.4.4",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "randomstring": "^1.2.2",
    "sib-api-v3-sdk": "^8.2.1",
    "slug": "^5.2.0",
    "tslib": "~2.3.1",
    "winston": "^3.3.3",
    "xlsx": "^0.18.5",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.13",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "~5.7.0",
    "@typescript-eslint/parser": "~5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "jest-html-reporter": "^3.6.0",
    "jest-sonar-reporter": "^2.0.0",
    "mongodb-memory-server": "^8.1.0",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "stream-buffers": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tsutils": "~3.21.0",
    "typescript": "^4.5.4"
  },
  "_moduleAliases": {
    "@server": "build",
    "@models": "build/models",
    "@config": "build/config",
    "@utils": "build/utils",
    "@validations": "build/validations",
    "@definitions": "build/definitions",
    "@middlewares": "build/middlewares",
    "@v1-routes": "build/v1/routes",
    "@v1-controllers": "build/v1/controllers",
    "@v1-definitions": "build/v1/definitions",
    "@v1-services": "build/v1/services",
    "@v1-validations": "build/v1/validations"
  }
}
